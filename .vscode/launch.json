    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: alerts from netld to livenx",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--alerts",
                "--fromproduct",
                "netld",
                "--toproduct",
                "livenx"
            ],
            "envFile": "${workspaceFolder}/LiveNX/integrations/.env"
        },
        {

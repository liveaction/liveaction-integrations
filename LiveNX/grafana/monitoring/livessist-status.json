          },
          "pluginVersion": "4.11.2",
          "queryType": "table",
          "rawSql": "WITH filtered AS (\n  SELECT\n    tenant_id,\n    chain_id,\n    `timestamp`,\n    role\n  FROM \"liveassist_chat\".\"chat_messages\"\n  WHERE\n    $__timeFilter(`timestamp`)\n    AND ('${TenantID}' = 'All' OR tenant_id = '${TenantID}')\n    AND ('${ChainID}'  = 'All' OR chain_id  = '${ChainID}')\n    AND role IN ('user', 'assistant')         -- only roles we care about\n),\nsorted AS (\n  SELECT\n    chain_id,\n    -- Array of tuples (ts, role) sorted by timestamp\n    arraySort(groupArray(tuple(`timestamp`, role))) AS evts\n  FROM filtered\n  GROUP BY chain_id\n)\nSELECT\n  pair.2.1 AS time,                                   -- assistant msg timestamp\n  dateDiff('second', pair.1.1, pair.2.1) AS value,    -- response time in seconds\n  chain_id AS metric\nFROM\n(\n  SELECT\n    chain_id,\n    arrayJoin(\n      arrayZip(\n        arrayPopBack(evts),    -- previous (ts, role)\n        arrayPopFront(evts)    -- current  (ts, role)\n      )\n    ) AS pair\n  FROM sorted\n)\nWHERE pair.1.2 = 'user' AND pair.2.2 = 'assistant'    -- user â†’ assistant only\nORDER BY time\nLIMIT 1000;\n\n\n\n",
          "refId": "A"
        }
      ],
      "title": "Chat Message Response Time (s)",
      "type": "timeseries"
    },

          "rawQuery": "SELECT \n    time AS Time,\n    8*(SUM(SumOctets)/5/60) AS value,\n    concat(ApplicationName, ':', toString(d.name)) AS metric\nFROM livenx_flowdb.basic_entity_app_5m_dist\nJOIN livenx_flowdb.dscp AS d ON toUInt8(Dscp) = d.dscp\nWHERE time >= 1708095952 AND time <= 1708096852 AND (ApplicationName, Dscp) GLOBAL IN \n(\n    SELECT\n        ApplicationName, Dscp\n    FROM livenx_flowdb.basic_entity_app_5m_dist\n    JOIN livenx_flowdb.dscp AS d ON toUInt8(Dscp) = d.dscp\n    WHERE time >= 1708095952 AND time <= 1708096852 AND \n    (('All' LIKE 'All') OR (ApplicationName LIKE 'All')) AND \n    (('All' LIKE 'All') OR (d.name LIKE 'All')) AND \n    (('All' LIKE 'All') OR (DeviceSerial LIKE 'All'))\n    GROUP BY ApplicationName, Dscp\n    ORDER BY SUM(SumOctets) DESC\n    LIMIT 10\n)\n\nGROUP BY Time,metric\nORDER BY Time",
          "rawSql": "SELECT\n    time AS Time,\n    8*(SUM(SumOctets)/${Granularity}/60) AS Traffic,\n    concat(ApplicationName, ':', toString(Dscp)) AS metric\nFROM livenx_flowdb.basic_entity_app_${Granularity}m_dist\nWHERE $__timeFilter(time) AND (ApplicationName, Dscp) GLOBAL IN \n(\n    SELECT\n        ApplicationName, toString(Dscp)\n    FROM livenx_flowdb.basic_entity_app_${Granularity}m_dist\n    WHERE $__timeFilter(time) AND \n    (('All' LIKE '${ApplicationName}') OR (ApplicationName LIKE '${ApplicationName}'))\n    GROUP BY ApplicationName, toString(Dscp)\n    ORDER BY SUM(SumOctets) DESC\n    LIMIT 10\n)\n\nGROUP BY Time,metric\nORDER BY Time",

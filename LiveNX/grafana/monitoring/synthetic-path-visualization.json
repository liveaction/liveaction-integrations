            }
          },
          "pluginVersion": "4.10.1",
          "queryType": "traces",
          "rawSql": "WITH hops AS (\n    SELECT\n        TraceId,\n        ServiceName,\n        SpanName,\n        arrayJoin(Events.Attributes) AS hop,\n        hop['hop'] AS hop_number,\n        hop['ip_address'] AS ip_address,\n        hop['location'] AS location,\n        hop['network'] AS network,\n        hop['prefix'] AS prefix,\n        hop['rdns'] AS rdns,\n        toInt32OrZero(hop['response_time_ms']) AS response_time_ms\n\n    FROM \"default\".\"otel_traces\" WHERE ( Timestamp >= $__fromTime AND Timestamp <= $__toTime ) AND (('All' LIKE '${PathID}') OR (SpanAttributes['path.id'] LIKE '${PathID}')) AND ( TraceId = '${TraceID}' )  AND ( SpanAttributes['test.name'] = '${TestName}' ) AND ( SpanAttributes['agent.name'] = '${AgentName}' ) ORDER BY Timestamp DESC\n)\n\nSELECT\n    ip_address AS id,\n    concat(\n        if(network != '', concat(' (', network, ')'), ''),\n        ' ',\n        ip_address\n    ) AS title,\n    10 AS nodeRadius\nFROM hops\nGROUP BY ServiceName, ip_address, network\n\n",
          "refId": "nodes"
        },
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "PDEE91DDB90597936"
          },
          "editorType": "sql",
          "format": 3,
          "hide": false,
          "meta": {
            "builderOptions": {
              "columns": [],
              "database": "",
              "limit": 1000,
              "mode": "list",
              "queryType": "table",
              "table": ""
            }
          },
          "pluginVersion": "4.3.2",
          "queryType": "traces",
          "rawSql": "\n  WITH trace_hops AS (\n      SELECT\n          TraceId,\n          ServiceName,\n          SpanName,\n          -- Events.Name and Events.Attributes are parallel arrays\n          arrayFilter((name, attr) -> name = 'hop_details',\n                     Events.Name, Events.Attributes) AS hop_events_filtered,\n          arrayFilter(attr ->\n                     arrayElement(Events.Name, indexOf(Events.Attributes, attr)) = 'hop_details',\n                     Events.Attributes) AS hop_attributes\n      FROM otel_traces\n      WHERE ( Timestamp >= $__fromTime AND Timestamp <= $__toTime ) AND (('All' LIKE '${PathID}') OR (SpanAttributes['path.id'] LIKE '${PathID}')) AND ( TraceId = '${TraceID}' )  AND ( SpanAttributes['test.name'] = '${TestName}' ) AND ( SpanAttributes['agent.name'] = '${AgentName}' )\n  ),\n  flattened_hops AS (\n      SELECT\n          TraceId,\n          ServiceName,\n          SpanName,\n          hop_attr AS hop_attr,\n          toInt32OrZero(hop_attr['hop']) AS hop_number,\n          hop_attr['ip_address'] AS ip_address,\n          hop_attr['location'] AS location,\n          hop_attr['network'] AS network,\n          toInt32OrZero(hop_attr['response_time_ms']) AS response_time_ms\n      FROM trace_hops\n      ARRAY JOIN hop_attributes AS hop_attr\n  ),\n  hop_pairs AS (\n      SELECT\n          h1.TraceId,\n          h1.hop_number AS hop_from_num,\n          h2.hop_number AS hop_to_num,\n          h1.ip_address AS source,\n          h2.ip_address AS target,\n          h1.response_time_ms AS source_latency,\n          h2.response_time_ms AS target_latency,\n          h1.location AS source_location,\n          h2.location AS target_location\n      FROM flattened_hops h1\n      INNER JOIN flattened_hops h2\n          ON h1.TraceId = h2.TraceId\n          AND h2.hop_number = h1.hop_number + 1\n  )\n  SELECT\n      source,\n      target,\n      concat(\n          source,\n          ' -> ',\n          target,\n          ' (',\n          toString(target_latency),\n          'ms)'\n      ) AS id,\n      target_latency AS mainstat\n  FROM hop_pairs\n  ORDER BY hop_from_num",
          "refId": "edges"
        }
      ],
      "title": "Path Visualization",
      "transparent": true,
      "type": "nodeGraph"
    },
